# 数据库配置
MYSQL_ROOT_PASSWORD=root123
MYSQL_DATABASE=wlweb
MYSQL_USER=wlweb
MYSQL_PASSWORD=wlweb123

# 后端配置
SECRET_KEY=your-secret-key-change-in-production-must-be-at-least-32-characters
DEBUG=false
CORS_ORIGINS=http://localhost:3000,http://localhost:5173,http://localhost:80

# 数据库连接URL（自动生成，通常不需要修改）
DATABASE_URL=mysql+pymysql://wlweb:wlweb123@mysql:3306/wlweb

# Redis配置
REDIS_URL=redis://redis:6379/0

# JWT配置
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# 应用配置
APP_NAME=游戏脚本中间件管理系统
APP_VERSION=1.0.0
MAX_TERMINALS=100
TASK_TIMEOUT=300
DATA_RETENTION_DAYS=30
